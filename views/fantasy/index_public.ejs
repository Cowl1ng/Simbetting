<%- include('_headers_public.ejs') %>
<%- include('_messages') %>
<br>
<h2>Games this week</h2>

<div class="container" role="navigation">
  <ul class="nav nav-pills">
    <li role="presentation" class="active; nav-link"><a href="/fantasy/list">This week</a></li>
    <li role="presentation" class="nav-link"><a href="/fantasy/list/nextweek">Next week</a></li>
  </ul>
</nav>
</div>
  <div id="row">
 <% fantasygames.forEach(fantasygame => {  %>
  <div class="col-md-3">
  <a href="/fantasy/<%= fantasygame.id %>"><%= fantasygame.team_a %> vs <%= fantasygame.team_b %></a>
  <h6><%= fantasygame.team_a %> to win: <%= fantasygame.odds_a %></h6>
  <h6><%= fantasygame.team_b %> to win: <%= fantasygame.odds_b %></h6>
  <h6>Draw : <%= fantasygame.odds_draw %></h6>
  </div>
 <% }) %>
</div>

